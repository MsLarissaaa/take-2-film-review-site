<div class="container-fluid text-container">
  <h1>Login</h1>



  <%= form_for :session, html: { class: "form-horizontal" } do |f| %>
    <div class="form-group error-messages">
      <% if @error_message %>
        <p><%= @error_message %></p>
      <% end %>
    </div>

    <div class="form-group text-right">
      <%= f.label :email, "Email", class: "col-md-4 control-label" %>
      <div class="col-md-5">
        <%= f.text_field :email, class: "form-control new-user-field" %>
      </div>
      <div class="col-md-3"></div>
    </div>

    <div class="form_group text-right">
      <%= f.label :password, "Password", class: "col-md-4 control-label" %>
      <div class="col-md-5">
        <%= f.password_field :password, class: "form-control new-user-field" %>
      </div>
      <div class="col-md-3"></div>
    </div>

    <div class="form_group text_right">
      <div class="col-md-4"></div>
      <div class= "col-md-4"><%= f.submit %></div>
      <div class="col-md-4 "></div>
    </div >




  <% end %>
</div>