<div class="container-fluid text-container">
  <div class="row">
    <div class="col-md-3"></div>
    <div class="col-md-6 text-center">
      <h1>Login</h1>
     </div>
    <div class="col-md-3"></div>
  </div>

  <div class="row main-row">
    <div class="col-md-6 popcorn">
      <%= image_tag "https://encrypted-tbn3.gstatic.com/images?q=tbn:ANd9GcSNI7ztTtTNFjUGgzrBJYHO9m1zQmMo41PSTOhvcLDhiSX-b-xVVw", :class => "users-image" %>
    </div>
    <%= form_for :session, html: { class: "form-horizontal" } do |f| %>
    <div class="col-md-6">
      <div class="form-group text-right e-log">
        <%= f.label :email, "Email", class: "col-md-2 control-label" %>
        <div class="col-md-6">
          <%= f.text_field :email, class: "form-control" %>
        </div>
        <div class="col-md-4"></div>
      </div>

      <div class="form_group text-right">
        <%= f.label :password, "Password", class: "col-md-2 control-label" %>
        <div class="col-md-6 pass-shift">
          <%= f.password_field :password, class: "form-control pass-log" %>
        </div>
        <div class="col-md-4"></div>
      </div>

      <div class="form_group text_right">
        <div class="col-md-4"></div>
        <div class= "col-md-6"><%= f.submit("Login", :class => "log-but") %></div>
        <div class="col-md-2"></div>
      </div >

    <% end %>
  </div>
  <div class="row">
    <div class="col-md-5"></div>
    <div class="form-group error-messages col-md-4 sess-er">
      <% if @error_message %>
        <p><%= @error_message %></p>
      <% end %>
    </div>
    <div class-"col-md-3"></div>
  </div>
</div>